{
    "name": "entidi/feedreader",
    "type": "silverstripe-vendormodule",
    "description": "A new page type that can access the data of an external feed",
    "homepage": "http://silverstripe.entidi.com/",
    "license": "BSD-2-Clause",
    "keywords": [
        "silverstripe",
        "feedreader",
        "feed",
        "rss",
        "atom",
        "page"
    ],
    "authors": [
        {
            "name": "Nicola Fontana",
            "email": "ntd@entidi.it",
            "homepage": "http://www.entidi.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "http://dev.entidi.com/p/silverstripe-feedreader/issues/",
        "source": "http://dev.entidi.com/p/silverstripe-feedreader/source/tree/master/"
    },
    "require": {
        "silverstripe/framework": "~4.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "eNTiDi\\FeedReader\\": "src/",
            "eNTiDi\\FeedReader\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "entidi/silverstrap": "The default template is compatible with the silverstrap theme"
    }
}
